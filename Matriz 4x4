import numpy as matriz
n_array =  matriz.array ([[12,2,5,11],
                        [21,30,16,29],
                        [32,47,9,10],
                        [36,42,19,28]])
print("matriz 4x4 :")
print(n_array)
det = matriz.linalg.det(n_array)
print("\nDeterminante da matriz Ã© :")
print(int(det))

